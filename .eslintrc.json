{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends":"airbnb",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console":"off",
        "max-classes-per-file":"off",
        "no-unused-vars":"off",
        "no-underscore-dangle":"off",
        "class-methods-use-this":"off",
        "no-shadow":"off",
        "max-len":"off",
        "no-mixed-operators":"off",
        "no-bitwise":"off",
        "eqeqeq":"off"
    }
}

